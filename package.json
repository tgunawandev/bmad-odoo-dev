  "repository": {
    "type": "git",
    "url": "git@github.com:tgunawandev/bmad-odoo-dev.git"
  },
  "bugs": {
    "url": "https://github.com/tgunawandev/bmad-odoo-dev/issues"
  },
  "homepage": "https://github.com/tgunawandev/bmad-odoo-dev#readme",
  "peerDependencies": {
    "bmad-method": ">=4.30.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "prettier": "^3.5.3"
  },
  "engines": {
