    "npm": ">=8.0.0"
  },
  "files": [
    "bin/",
    "dist/",
    "scripts/",
    "agents/",
    "tasks/",
    "templates/",
    "checklists/",
    "data/",
    "teams/",
    "config.yaml",
    "index.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "prepare": "npm run build",
    "validate": "node scripts/validate.js",
    "publish-manual": "node scripts/publish.js"
